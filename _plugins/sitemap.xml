<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for post in site.sitemap_posts %}
    <url>
        <loc>{{ post.url | prepend: site.url }}</loc>
        <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    </url>
    {% endfor %}
</urlset>